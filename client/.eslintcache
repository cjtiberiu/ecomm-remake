          <Select\r\n                        style={{ width: 200 }}\r\n                        name='category'\r\n                        placeholder=\"Select a category\"\r\n                        optionFilterProp=\"name\"\r\n                        value={product.category}\r\n                        onChange={value => setProduct({ ...product, subs: [], category: value })}\r\n                    >\r\n                        {\r\n                            !categories ? null : (\r\n                                categories.map(el => {\r\n                                    return <Option key={el._id} value={`${el._id}`}>{el.name}</Option>\r\n                                })\r\n                            )\r\n                        }\r\n                    </Select>\r\n                </Form.Item>\r\n\r\n\r\n                <Form.Item label='Product sub-category'>\r\n                    <Select\r\n                        mode='multiple'\r\n                        style={{ width: 200 }}\r\n                        placeholder=\"Select a sub-category\"\r\n                        optionFilterProp=\"children\"\r\n                        value={product.subs.map(el => el)}\r\n                        onChange={value => setProduct({ ...product, subs: value })}\r\n                    >\r\n                        {\r\n                            !subs ? null : (\r\n                                subs.map(el => {\r\n                                    if (el.parent === product.category) {\r\n                                        return <Option key={el._id} value={`${el._id}`}>{el.name}</Option>\r\n                                    }\r\n                                })\r\n                            )\r\n                        }\r\n                    </Select>\r\n                </Form.Item>\r\n\r\n\r\n                <Form.Item label='Product Quantity'>\r\n                    <InputNumber value={product.quantity} onChange={value => setProduct({ ...product, quantity: value })} />\r\n                </Form.Item>\r\n                \r\n\r\n                <Form.Item label=\"Shipping\">\r\n                    <Radio.Group value={product.shipping} onChange={e => setProduct({ ...product, shipping: e.target.value })}>\r\n                        <Radio.Button value=\"Yes\">Yes</Radio.Button>\r\n                        <Radio.Button value=\"No\">No</Radio.Button>\r\n                    </Radio.Group>\r\n                </Form.Item>     \r\n\r\n\r\n                <Form.Item label='Product color'>\r\n                    <Select\r\n                        style={{ width: 200 }}\r\n                        placeholder=\"Select a color\"\r\n                        optionFilterProp=\"children\"\r\n                        value={product.color}\r\n                        onChange={value => setProduct({ ...product, color: value })}\r\n                    >\r\n                        {\r\n\r\n                            colors.map(el => {\r\n                                return <Option key={el} value={`${el}`}>{el}</Option>\r\n                            })\r\n                        }\r\n                    </Select>\r\n                </Form.Item>   \r\n\r\n\r\n                <Form.Item label='Product brand'>\r\n                    <Select\r\n                        style={{ width: 200 }}\r\n                        placeholder=\"Select a brand\"\r\n                        optionFilterProp=\"children\"\r\n                        value={product.brand}\r\n                        onChange={value => setProduct({ ...product, brand: value })}\r\n                    >\r\n                        {\r\n\r\n                            brands.map(el => {\r\n                                return <Option key={el} value={`${el}`}>{el}</Option>\r\n                            })\r\n                        }\r\n                    </Select>\r\n                </Form.Item> \r\n\r\n                <Form.Item label='Upload Images'>\r\n                    <ImageUpload values={product} setValues={setProduct} setLoading={setLoading} loading={loading} />\r\n                </Form.Item>\r\n                \r\n               \r\n                <button onClick={handleSubmit} className='btn btn-outline-dark mt-3' type='primary'>Edit product</button>\r\n\r\n            </Form>\r\n\r\n\r\n        </div>\r\n    )\r\n};\r\n\r\nexport default EditProduct;","C:\\Users\\claud\\Desktop\\projects\\ecommerce\\client\\src\\pages\\admin\\products\\ListProducts.js",["764"],"import React from 'react';\r\nimport { useHistory } from 'react-router-dom';\r\nimport { filteredArray } from '../../../utility/filters';\r\nimport ProductCard from '../../../components/cards/ProductCard';\r\n\r\nconst ListProducts = props => {\r\n\r\n    const { products, keyword} = props;\r\n    const history = useHistory();\r\n\r\n    return (\r\n        <div className='row mt-2 '>\r\n                <div className='col-md-12 d-flex flex-wrap' style={{ overflowY: 'scroll', maxHeight: '70vh'}}>\r\n                    {\r\n\r\n                        filteredArray(products, keyword).map(el => {\r\n                            return (\r\n\r\n                                <ProductCard type='admin' key={el._id} product={el} />\r\n                                \r\n                                // <div className='border bg-light mb-1 d-flex justify-content-between align-items-center' key={el.slug}>\r\n                                //     <div className='d-flex align-items-center w-25'>\r\n\r\n                                //         <img src={el.images[0].url} style={{ width: '70px', height: 'auto'}} />\r\n                                //         <div className='ml-2'>{el.title}</div>\r\n\r\n                                //     </div>\r\n\r\n                                //     <div>Stock: {el.quantity} </div>\r\n                                //     <div className='d-flex align-items-center'>\r\n                                //         <button \r\n     