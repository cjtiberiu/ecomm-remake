  dispatch({\r\n                  type: \"USER_LOADED\",\r\n                  payload: {\r\n                    name: res.data.name,\r\n                    email: res.data.email,\r\n                    token: idTokenResult.token,\r\n                    role: res.data.role,\r\n                    _id: res.data._id,\r\n                    address: res.data.address\r\n                  },\r\n                });\r\n              })\r\n              .catch((err) => console.log(err));\r\n\r\n\t\t\t  // get user cart items from db usign firebase token\r\n              getCartItems(idTokenResult.token)\r\n                .then(res => {\r\n                \tdispatch({ \r\n\t\t\t\t\t\ttype: 'GET_CART_ITEMS', \r\n\t\t\t\t\t\tpayload: res.data\r\n\t\t\t\t\t})\r\n\t\t\t\t})\r\n\t\t\t\t.catch(err => console.log(err))\r\n          }\r\n\t\t});\r\n\t\t\r\n        // cleanup\r\n\t\treturn () => unsubscribe();\r\n\t\t\r\n      }, [dispatch]);\r\n\r\n\t\r\n\t// REDUX - get the number of total products from backend\r\n    useEffect(() => {\r\n        getProductsCount()\r\n\t\t\t.then(res => {\r\n\t\t\t\tdispatch({ type: 'GET_PRODUCTS_COUNT', payload: res.data })\r\n\t\t\t})\r\n\t\t\t.catch(err => console.log(err))\r\n\t}, [getProductsCount])\r\n\t\t\r\n\r\n    return (\r\n\r\n\t\t<div>\r\n\t\t\t<div className='nav-container' style={{ backgroundColor: 'rgb(33,133,208)'}}>\r\n\t\t\t\t<Header />\r\n\t\t\t\t<MobileNav />\r\n\t\t\t</div> \r\n\r\n\t\t\t<div className='container-fluid'>\r\n             \r\n\t\t\t\t<ToastContainer></ToastContainer>\r\n\t\t\t\t<Switch>\r\n\t\t\t\t\t\r\n\t\t\t\t\t<ErrorBoundary>\r\n\t\t\t\t\t\t<Suspense fallback={<Spinner />}>\r\n\t\t\t\t\t\t\t<Route exact path='/' component={HomePage} />\r\n\t\t\t\t\t\t\t<Route exact path='/products' component={ShopPage} />\r\n\t\t\t\t\t\t\t<Route exact path='/login' component={Login} />\r\n\t\t\t\t\t\t\t<Route exact path='/products/:id' component={ProductPage} />\r\n\t\t\t\t\t\t\t<Route exact path='/cart' component={CartPage} 